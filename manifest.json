{
  "manifest_version": 2,
  "name": "Abioka resim yükleyici",
  "description": "Beyaz yakalının, hizliresim ve imgur resimlerini görmenisini sağlar.",
  "version": "1.0",
  "icons": {"16": "icon16.png", "48": "icon64.png", "128": "icon128.png"},
  "author": "abioka.com",
  "homepage_url": "http://abioka.com",
  "content_scripts": [
   {
     "matches": ["http://*/*", "https://*/*"],
     "css": ["core.css"],
     "js": ["jquery-3.1.0.min.js", "core.js"],
     "run_at": "document_end",
     "all_frames": true
   }
 ]
}
