{
  "manifest_version": 2,
  "name": "Abioka resim yükleyici",
  "description": "hizliresim ve imgur resimlerini görmenizi sağlar.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
   {
     "matches": ["http://*/*", "https://*/*"],
     "css": ["core.css"],
     "js": ["jquery-3.1.0.min.js", "core.js"],
     "run_at": "document_end",
     "all_frames": true
   }
 ]
}
